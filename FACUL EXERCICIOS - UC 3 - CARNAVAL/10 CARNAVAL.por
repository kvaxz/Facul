programa {
  funcao inicio() {

    inteiro n1, n2

    escreva("Informe o primeiro número: ")
    leia(n1)

    escreva("Informe o segundo número: ")
    leia(n2)

    se(n1>n2){
      escreva(n1," é maior do que ",n2)
    }
    senao se(n2>n1){
      escreva(n2," é maior do que ",n1)
    }
    
  }
}
