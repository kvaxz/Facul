programa {
  funcao inicio() {
    inteiro idade

    escreva("Informe a idade para verificação de disponibilidade de desconto: ")
    leia(idade)

    se(idade>=60){
      escreva("Direito a um desconto de 20% no restaurante NATIVAS")  
    }
    senao{
      escreva("Não possui direito a um desconto de 20% no restaurante NATIVAS")
    }
    
  }
}
